<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/basic3"
    >
    <RelativeLayout
        android:id="@+id/map_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="150dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="250dp"
        >

    </RelativeLayout>
    ...


    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"

        android:gravity="center"

        android:layout_marginEnd="78dp"
        android:background="@color/white"
        android:hint="회사명, 거리명 입력" />

    <android.widget.Button
        android:id="@+id/button2"
        android:layout_width="100dp"
        android:gravity="center"
        android:layout_height="35dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round2"
        android:layout_marginTop="65dp"
        android:text="주거시설" />

    <android.widget.Button
        android:id="@+id/button3"
        android:layout_width="100dp"
        android:gravity="center"
        android:layout_height="35dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="65dp"
        android:background="@drawable/button_round2"
        android:text="재활시설" />

    <android.widget.Button
        android:id="@+id/button4"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="65dp"
        android:background="@drawable/button_round2"
        android:gravity="center"
        android:text="교육시설"
        android:textColor="@color/black"
        app:strokeColor="#F8F8F8" />


    <EditText
        android:id="@+id/et_addr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="600dp"
        android:hint="표시할 주소 입력"
         />

    <Button
        android:id="@+id/btn_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="650dp"

        android:text="표시!" />

    <EditText
        android:id="@+id/et_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="700dp"
        android:textSize="8dp"

        android:hint="txt1" />


    <Button
        android:id="@+id/btn_mypos"
        android:text="내 위치정보 가져오기"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="670dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/txt_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="730dp"
        android:textSize="8dp"

        />


    <Button
        android:id="@+id/btn_find"
        android:text="길찾기"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="755dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_search" />


</RelativeLayout>